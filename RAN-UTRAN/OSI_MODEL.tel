function OSI()

LOG_SQUARE("RANAP");
LOG_SQUARE("SCCP");
LOG_SQUARE("M3UA");
LOG_SQUARE("SCTP/IP");

LOG_SQUARE("RANAP\nSCCP\nM3UA\nSCTP/IP\n");

endfunction

