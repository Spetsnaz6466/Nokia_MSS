// Esta macro valida la creacion de un usuario via SSH login

function main()
 string USER,PASS,MSS_IP;
 integer x,y;
 
 y=277;
 PASS="123456"; 
 MSS_IP="10.66.67.193";
 device("MSSARA02");
 for(x=1; x<=y; x++)
   USER="putty " + Usuarios(x) + "@" + MSS_IP + " -pw " + PASS;
   print("*%d/%d %s\n",x,y,USER);
   tx("ZIAQ:TYPE=ALL:;");
   winexec(USER); 
   pause(0);  
   tx("ZIAQ:TYPE=ALL:;");
 endfor
endfunction

